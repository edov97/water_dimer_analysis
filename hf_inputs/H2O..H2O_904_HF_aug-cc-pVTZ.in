import psi4

# Set memory and output.
psi4.set_memory('500 MB')
psi4.core.set_output_file('H2O..H2O_904_HF_output.dat', False)

# Set options for HF:
psi4.set_options({
    'basis': 'aug-cc-pVTZ',
    'scf_type': 'pk',
    'freeze_core': True,
    'e_convergence': 1e-9,
    'd_convergence': 1e-8,
})

# Define the molecule.
molecule H2O..H2O_904 {
units Bohr
no_com
no_reorient
0 1
--
0 1
   O1              0.00000000       0.00000000       0.00000000
   H1             -1.45365196       0.00000000      -1.12168732
   H2              1.45365196       0.00000000      -1.12168732
--
0 1
   O1             -4.36123709       0.00000000      -2.64814461
   H1             -5.40457040       1.45365242      -2.23627486
   H2             -5.40457289      -1.45365150      -2.23627791
}

# Perform a supramolecular HF energy calculation.
energy('SCF')
