import psi4

# Set memory and output.
psi4.set_memory('500 MB')
psi4.core.set_output_file('H2O..H2O_913_HF_output.dat', False)

# Set options for HF:
psi4.set_options({
    'basis': 'aug-cc-pVTZ',
    'scf_type': 'pk',
    'freeze_core': True,
    'e_convergence': 1e-9,
    'd_convergence': 1e-8,
})

# Define the molecule.
molecule H2O..H2O_913 {
units Bohr
no_com
no_reorient
0 1
--
0 1
   O1              0.00000000       0.00000000       0.00000000
   H1             -1.45365196       0.00000000      -1.12168732
   H2              1.45365196       0.00000000      -1.12168732
--
0 1
   O1             -8.07636497       0.00000000      -4.90397150
   H1             -9.11969829       1.45365242      -4.49210174
   H2             -9.11970077      -1.45365150      -4.49210479
}

# Perform a supramolecular HF energy calculation.
energy('SCF')
